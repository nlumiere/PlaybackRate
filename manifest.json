{
  "manifest_version": 3,
  "name": "PlaybackRate",
  "description": "This is a sample Chrome extension with a popup.",
  "version": "0.1.0",
  "permissions": ["activeTab", "tabs", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "what is even u"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}
